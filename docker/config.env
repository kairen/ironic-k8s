IPMI_RANGE=192.168.20.10,192.168.20.20,12h
DATA_RANGE=192.168.30.10,192.168.30.20,12h
BOOT_IPXE_ENDPOINT="http://${HOST_IP}:8080/boot.ipxe"

# Ironic Settings
IRONIC_DEBUG=false
