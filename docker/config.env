# Host ip address
HOST_IP=192.168.30.6

# RabbitMQ credentials
RABBITMQ_DEFAULT_USER=guest
RABBITMQ_DEFAULT_PASS=rabbit_secret

# MariaDB credentials
MYSQL_ROOT_PASSWORD=sql_secret
MYSQL_IRONIC_PASSWORD=ironic_secret

# dnsmasq settings
IPMI_INTERFACE=enp0s8
